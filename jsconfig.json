{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "#config/*": ["src/config/*"],
      "#core/*": ["src/core/*"],
      "#helper/*": ["src/helper/*"],
      "#routes/*": ["src/routes/*"],
      "#middlewares/*": ["src/middlewares/*"],
      "#models/*": ["src/models/*"],
      "#services/*": ["src/services/*"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules", "**/node_modules/*"]
}
